#/usr/local/bin/bash

startDjango() {
	/usr/local/bin/python $1/manage.py runserver $2
}

if [ $1 ]; then
	echo "Usage: ./start_cssef"
	exit
fi

webUrl="0.0.0.0:8080"
webLogPath="web_interface.log"

apiUrl="0.0.0.0:8000"
apiLogPath="scoring_engine.log"

startDjango "WebInterface" $webUrl $webLogPath &
startDjango "ScoringEngine" $apiUrl $apiLogPath &